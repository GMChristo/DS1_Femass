	<queryString>
		<![CDATA[SELECT oc.codigo "codigo",
       oc.data "data",
       oc.valor "valor",
       oc.condPag "condPag",
       fu.nome "user_comprador.nome",
       fo.razao "fornecedor.razao",
       fo.cpf_cnpj "fornecedor.cpf_cnpj",
       fo.contato "fornecedor.contato"
FROM [financeiro].[dbo].[tbl_ordemcompra] oc
INNER JOIN [financeiro].[dbo].[tbl_funcionarios] fu ON oc.user_comprador_codigo = fu.codigo
INNER JOIN [financeiro].[dbo].[tbl_fornecedor] fo ON oc.fornecedor_codigo = fo.codigo
WHERE oc.codigo = $P{CODIGO_OC}]]>
	</queryString>
